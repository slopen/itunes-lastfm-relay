{
  "name": "itunes-lastfm-relay",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:production": "npm run lint && npm run build:client:production && npm run build:server:production",
    "build:client": "NODE_ENV=development webpack --config scripts/webpack/config.js -d",
    "build:server": "NODE_ENV=development ./node_modules/.bin/babel -d build/server src/server",
    "build:relay": "relay-compiler --src src --schema data/schema.graphql",
    "build:client:production": "NODE_ENV=production webpack --config scripts/webpack/config.js -d",
    "build:server:production": "NODE_ENV=production ./node_modules/.bin/babel -d build/server src/server",
    "dev:client": "NODE_ENV=development webpack-dev-server --config scripts/webpack/config.js --hot --content-base build",
    "dev:server": "NODE_ENV=development nodemon --watch src/server --watch config src/server/index.js --exec babel-node",
    "dev:relay": "relay-compiler --src src --schema data/schema.graphql --watch",
    "lint": "eslint src",
    "server": "NODE_ENV=development node build/server",
    "server:production": "NODE_ENV=production node build/server",
    "start": "npm run server:production",
    "dev": "npm run dev:server & npm run dev:relay & npm run dev:client",
    "production": "npm run build:production && npm start",
    "generate:ssl": "nginx/generate-ssl.sh",
    "test:flow": "flow check --show-all-errors src",
    "coverage:flow": "flow-coverage-report"
  },
  "keywords": [],
  "author": "kostrov@gmail.com",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-relay": "^1.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.3",
    "relay-compiler": "^1.5.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@risingstack/graffiti-mongoose": "^6.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "body-parser": "^1.18.2",
    "classnames": "2.2.5",
    "config": "^1.30.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.0",
    "moment": "^2.14.1",
    "mongoose": "^4.5.7",
    "process-env": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-list": "^0.8.0",
    "react-relay": "^1.5.0",
    "react-relay-network-modern": "^2.1.1",
    "react-relay-network-modern-ssr": "^0.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "regenerator-runtime": "^0.11.1",
    "relay-context-provider": "^1.0.4",
    "relay-query-lookup-renderer": "^3.0.1",
    "relay-runtime": "^1.5.0",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4"
  }
}
