{
	"plugins": [
		["module-resolver", {
      		"root": ["./src"]
    	}],
		["relay", {
			"schema": "data/schema.graphql"
		}],
		["transform-runtime", {
			"polyfill": false,
			"regenerator": true
		}]
	],
	"presets": [
	    ["env", {
	      "targets": {
	      	"node": "8.1",
	        "browsers": ["last 2 versions"]
	      }
	    }],
		"stage-1",
		"react"
	]
}